<%= simple_form_for([ school, review ],
                    data: {
                      insert_in_list_target: 'form',
                      action: 'submit->insert-in-list#send'
                    }) do |f| %>
  <%= f.input :content, as: :text %>
  <%= f.input :rating, collection: (1..5).to_a, label: "Rate your experience" %>
  <%= f.button :submit %>
<% end %>
